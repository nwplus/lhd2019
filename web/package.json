{
  "name": "nwplus2019",
  "description": "The static websites for nwPlus 2019",
  "repository": "github:nwplus/nwplus2019",
  "author": "nwPlus 2019",
  "license": "MIT + Apache 2.0",
  "main": "index.js",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --hot --inline --watch",
    "build": "cross-env NODE_ENV=production webpack --config webpack.prod.js",
    "serve": "http-server ../docs -a localhost -p 8081 -c-1",
    "test": "cross-env NODE_ENV=development jest",
    "lint": "eslint . --ignore-pattern node_modules/"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/__mocks__/fileMock.js",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.js",
      "!**/node_modules/**",
      "!**/*.test.js",
      "!webpack.*",
      "!**/coverage/**",
      "!**/tests/**"
    ],
    "coverageReporters": [
      "text",
      "json"
    ],
    "setupFiles": [
      "<rootDir>/tests/setup.js"
    ]
  },
  "dependencies": {
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.6",
    "babel-loader": "7.1.5",
    "babel-minify-webpack-plugin": "0.3.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "cross-env": "5.2.0",
    "css-loader": "1.0.0",
    "dotenv": "6.0.0",
    "dotenv-webpack": "1.5.7",
    "history": "4.7.2",
    "html-webpack-plugin": "3.2.0",
    "node-sass": "4.9.0",
    "prop-types": "15.6.2",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "sass-loader": "7.1.0",
    "style-loader": "0.21.0",
    "url-loader": "1.0.1",
    "webpack": "4.16.3",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "3.1.5"
  },
  "devDependencies": {
    "babel-jest": "23.2.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "5.0.1",
    "eslint-config-airbnb": "17.0.0",
    "eslint-plugin-babel": "5.1.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jest": "21.17.0",
    "eslint-plugin-jsx-a11y": "6.1.0",
    "eslint-plugin-react": "7.10.0",
    "http-server": "^0.11.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.3.0",
    "react-test-renderer": "16.4.1"
  }
}
